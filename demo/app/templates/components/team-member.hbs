<div class="col-xs-12 col-sm-6 col-md-3 col-lg-3 col-xl-3">
	<div class="members">
		<img src="{{member.avatar}}" class="img-responsive img-circle img-member" alt="">
		<h3>{{member.name}}</h3>
		<h4>{{member.designation}}</h4>
		<p class="text-muted">{{member.about}}</p>
		<ul class="list-inline social-buttons">
			<li><a href="{{member.facebook}}"><i class="fa fa-facebook"></i></a>
			</li>
			<li><a href="{{member.twitter}}"><i class="fa fa-twitter"></i></a>
			</li>
			<li><a href="{{member.linkedin}}"><i class="fa fa-linkedin"></i></a>
			</li>
		</ul>
	</div>
</div>